Hansson2013_data = dataobj{

    DECLARED_VARIABLES{
        VEGF_obs
        VEGFR2_obs
        VEGFR3_obs 
        SKIT_obs
    } 
    
   DATA_INPUT_VARIABLES{
      ID : { use=id }
      CYCL : { type=categorical } 
      TIME : { use=idv, units="h" }
      DAYS : { type=continuous, units="day" }
      DV : { use=dv, prediction= VEGF_obs when FLAG==5,
                                 VEGFR2_obs when FLAG==6,
                                 VEGFR3_obs when FLAG==7,
                                 SKIT_obs when FLAG==8 ; }
      FLAG : { type=categorical } # 1=Dose ; 4=Tumor size (SLD); 5=VEGF ; 6=VEGFR2 ; 7=VEGFR3 ; 8=SKIT
      
      DVX : { type=continuous, units="mg/L" }
      DOS : { use=amt, units="mg" }
      PLA : { type=categorical } # placebo=1, treated=0
      CL : { use=covariate, type=continuous, units="L/h"}
      EVID : { type=categorical }
   }
   
   SOURCE{
       file="BIOMARKER_exDATA.csv"
       inputformat=nonmemFormat
       ignore="#"
   }
}